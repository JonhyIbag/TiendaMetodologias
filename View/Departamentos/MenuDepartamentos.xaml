<Window x:Class="Practica1.View.MenuDepartamentos"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Practica1.View"
        mc:Ignorable="d"
        Title="MenuDepartamentos" Height="550" Width="500">
    <StackPanel>
        <Button Content="Dar de alta un departamento" Click="BtnAlta" HorizontalAlignment="Center" Margin="0,20,0,0" VerticalAlignment="Top" Height="31" Width="226"/>
        <Button Content="Consultar Departamentos" Click="BtnConsultar" HorizontalAlignment="Center" Margin="0,20,0,0" VerticalAlignment="Top" Height="31" Width="226"/>
        <Button Content="Dar de baja un departamento" Click="BtnBaja" HorizontalAlignment="Center" Margin="0,20,0,0" VerticalAlignment="Top" Height="31" Width="226"/>
        <Button Content="Salir" Click="BtnSalir" HorizontalAlignment="Center" Margin="0,20,0,0" VerticalAlignment="Top" Height="31" Width="226"/>
        <Frame x:Name="FrameBotones" Height="250" Width="460" Margin="20,20,20,20"/>
    </StackPanel>
</Window>
